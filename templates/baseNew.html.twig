<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
           <!-- Basic -->
      <meta charset="utf-8" />
      <meta http-equiv="X-UA-Compatible" content="IE=edge" />
      <!-- Mobile Metas -->
      <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
      <!-- Site Metas -->
      <meta name="keywords" content="" />
      <meta name="description" content="" />
      <meta name="author" content="" />
      <link rel="shortcut icon" href= "{{asset("assets2/images/favicon.png")}}" type="">
      <title>Famms - Fashion HTML Template</title>
      <!-- bootstrap core css -->
      <link rel="stylesheet" type="text/css" href="{{asset("assets2/css/bootstrap.css")}}"/>
      <!-- font awesome style -->
      <link href="{{asset("assets2/css/font-awesome.min.css")}}" rel="stylesheet" />
      <!-- Custom styles for this template -->
      <link href="{{asset("assets2/css/style.css")}}" rel="stylesheet" />
      <!-- responsive style -->
      <link href="{{asset("assets2/css/responsive.css")}}" rel="stylesheet" />
        {% endblock %}

        {% block javascripts %}
            <!-- jQery -->
      <script src="{{asset("assets2/js/jquery-3.4.1.min.js")}}"></script>
      <!-- popper js -->
      <script src="{{asset("assets2/js/popper.min.js")}}"></script>
      <!-- bootstrap js -->
      <script src="{{asset("assets2/js/bootstrap.js")}}"></script>
      <!-- custom js -->
      <script src="{{asset("assets2/js/custom.js")}}"></script>
        {% endblock %}
    </head>
    <body>
     {% include('acc/header.html.twig') %}
        {% block body %}
        {% endblock %}
     {% include('acc/footer.html.twig') %}
    </body>
</html>
